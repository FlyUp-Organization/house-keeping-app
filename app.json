{
  "expo": {
    "name": "house-keeping",
    "slug": "house-keeping",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/splash-screen.png",
    "scheme": "house-keeping",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },

    "android": {
      "package": "com.bryan_flyup.housekeeping",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/splash-screen.png",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/splash-screen.png"
    },

    "plugins": ["expo-router", "expo-splash-screen", "expo-font"],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "bcd8a6bd-607f-4486-87a2-56efc8975869"
      }
    }
  }
}
